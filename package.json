{
  "name": "legit-scheduler",
  "version": "0.3.0",
  "description": "A React drag and drop scheduler component",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run compile",
    "test": "mocha --opts ./mocha.opts; eslint ./src/ --ext .jsx,.js --global require,exports:true",
    "compile": "babel src --ignore __tests__ --stage 0 --out-dir lib; node-sass src/css/default.scss lib/css/default.css",
    "example": "webpack-dev-server --config ./example/webpack.config.js --hot",
    "push": "npm run compile; npm publish ./"
  },
  "author": "Darin Haener <dphaener@gmail.com> (https://github.com/dphaener)",
  "license": "MIT",
  "keywords": [
    "react-component",
    "react",
    "calendar",
    "scheduler",
    "gantt"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/legitcode/scheduler.git"
  },
  "dependencies": {
    "immutable": "3.8.1",
    "legit-rubyfill": "0.0.7",
    "moment": "2.18.0",
    "moment-timezone": "0.5.11",
    "react": "15.4.2",
    "react-dnd": "2.2.4",
    "react-dnd-html5-backend": "2.2.4",
    "react-dom": "15.4.2",
