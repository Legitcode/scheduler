    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "chai": "3.5.0",
    "css-loader": "0.27.3",
    "eslint": "3.18.0",
    "eslint-plugin-react": "6.10.2",
    "estraverse-fb": "1.3.1",
    "expect": "1.20.2",
    "file-loader": "0.10.1",
