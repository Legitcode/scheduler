{
  "name": "legit-scheduler",
  "version": "0.0.2",
  "description": "A React drag and drop scheduler component",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --opts ./mocha.opts; eslint ./src/ --ext .jsx,.js --global require,exports:true",
    "compile": "babel src --ignore __tests__ --stage 0 --out-dir lib; node-sass src/css/default.scss lib/css/default.css",
    "example": "webpack-dev-server --config ./example/webpack.config.js --hot",
    "push": "npm run compile; npm publish ./"
  },
  "author": "Darin Haener <dphaener@gmail.com> (https://github.com/dphaener)",
  "license": "MIT",
  "keywords": [
    "react-component",
    "react",
    "calendar",
    "scheduler",
    "gantt"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/legitcode/scheduler.git"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "moment": "^2.11.1",
    "react": "^0.14.6",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.2",
    "react-dom": "^0.14.6",
    "react-draggable": "^1.1.3",
    "react-redux": "^4.0.6",
    "redux": "^3.0.6",
    "strftime": "^0.9.2"
  },
  "devDependencies": {
    "babel": "5.4.7",
    "babel-core": "5.4.7",
    "babel-eslint": "^4.1.1",
    "babel-loader": "5.1.3",
    "chai": "^3.2.0",
    "css-loader": "0.14.4",
    "eslint": "^0.24.1",
    "eslint-plugin-react": "^2.7.0",
    "expect": "1.8.0",
    "file-loader": "0.8.4",
    "img-loader": "1.1.0",
    "legit-tests": "^1.0.0",
    "mocha": "2.2.5",
    "mocha-babel": "3.0.3",
    "node-jsdom": "3.1.5",
    "node-libs-browser": "0.5.2",
    "node-sass": "^3.4.2",
    "react-hot-loader": "1.2.7",
    "react-tools": "0.13.3",
    "sass-loader": "1.0.2",
    "sinon": "1.15.4",
    "sinon-chai": "2.8.0",
    "style-loader": "0.12.3",
    "timekeeper": "0.0.5",
